                <ui:Button text="Retry" display-tooltip-when-elided="true" name="retry" style="width: 55%; height: 60px; font-size: 56px; background-color: rgb(243, 163, 97); border-top-left-radius: 13px; border-bottom-left-radius: 13px; border-top-right-radius: 13px; border-bottom-right-radius: 13px;" />
            </ui:VisualElement>
        </ui:VisualElement>
    </ui:VisualElement>
